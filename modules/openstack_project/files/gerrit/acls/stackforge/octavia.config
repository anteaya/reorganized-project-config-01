[access "refs/heads/*"]
        label-Code-Review = -2..+2 group octavia-core
        label-Workflow = -1..+1 group octavia-core
        abandon = group octavia-core
[access "refs/tags/*"]
        pushSignedTag = group octavia-ptl
[receive]
        requireChangeId = true
        requireContributorAgreement = true
[submit]
        mergeContent = true
