[access "refs/heads/*"]
        label-Code-Review = -2..+2 group cloudkitty-core
        label-Workflow = -1..+1 group cloudkitty-core
        abandon = group cloudkitty-core
[access "refs/tags/*"]
        pushSignedTag = group cloudkitty-ptl
[receive]
        requireChangeId = true
        requireContributorAgreement = true
[submit]
        mergeContent = true
